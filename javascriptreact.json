{
    /*
    	// Place your snippets for JavaScript React here. Each snippet is defined under a snippet name and has a prefix, body and 
    	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
    	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
    	// same ids are connected.
    	// Example:
    	"Print to console": {
    		"prefix": "log",
    		"body": [
    			"console.log('$1');",
    			"$2"
    		],
    		"description": "Log output to console"
    	}
	*/
    "comment_template": {
        "prefix": "comment",
        "body": [
            "/**",
            " * ${1:Description}",
            " * @param obj",
            " */"
        ],
        "description": "comment template typescript"
    },
    "Files Desciption": {
        "prefix": "starter",
        "body": [
            "/*",
            "File Name: ${1:File Name.}",
            "Description: ${2:Description of the file.}",
            "Author: ${3:Naieem Mahmud Supto}",
            "Version: ${4:Version}",
            "*/"
        ],
        "description": "File description at the start of file"
    },
    "Subsection block": {
        "prefix": "comment sub section",
        "body": [
            "//----------- ${1:Section Name}------------//"
        ],
        "description": "comment for subsections"
    },
    "Comment block": {
        "prefix": "comment block section",
        "body": [
            "// ======================================",
            "// ${1:main variable declaration block}",
            "// ======================================"
        ],
        "description": "comment for block sections"
    },
    "File Starter": {
        "prefix": "comment file starter",
        "body": [
            "/**",
            " * Project Name: ${1:Typescript todo}",
            " * Description: ${2:This is a description of the file in brief}",
            " * Author: ${3:Naieem Mahmud Supto}",
            " * Author URI: ${4:http://naieem.me/}",
            " * Repository: ${5:http://github.com/naieem/}",
            " * Version: ${6:1.2.4}",
            " * License: ${7:GPL2 or later}",
            " * License URI: ${8:https://www.gnu.org/licenses/gpl-2.0.html}",
            " */"
        ],
        "description": "File description at the start of file of project"
    }
}